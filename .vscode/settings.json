{
	"files.watcherExclude": {
		"**/routeTree.gen.ts": true,
    "**/dist": true,
    "**/.turbo": true
	},
	"search.exclude": {
    "**/routeTree.gen.ts": true,
    "**/dist": true,
    "**/.turbo": true
	},
	"files.readonlyInclude": {
    "**/routeTree.gen.ts": true,
    "**/.turbo": true
	},
  "files.exclude": {
    "**/.turbo": true,
    "**/.swc": true
  },
	"[javascript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[typescript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[json]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[html]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[css]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"editor.codeActionsOnSave": {
		"source.organizeImports.biome": "explicit"
	}
}
